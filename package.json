{
  "name": "savage-movie",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "docker:dev": "bash ./scripts/docker-dev.sh",
    "docker:restart": "bash ./scripts/docker-dev.sh restart",
    "docker:rebuild": "bash ./scripts/docker-dev.sh rebuild",
    "review": "coderabbit --type uncommitted --plain",
    "review:all": "coderabbit --type all --plain",
    "review:committed": "coderabbit --type committed --plain",
    "review:main": "coderabbit --base main --plain",
    "review:prompt": "coderabbit --type uncommitted --prompt-only"
  },
